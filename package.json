{
  "name": "enpractice",
  "displayName": "EnPractice",
  "description": "English Practice Extension for VS Code",
  "version": "1.0.0",
  "publisher": "enpractice",
  "repository": {
    "type": "git",
    "url": "https://github.com/enpractice/enpractice.git"
  },
  "keywords": ["english", "practice", "vocabulary", "learning"],
  "license": "MIT",
  "icon": "media/en.png",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "enpractice",
          "title": "EnPractice",
          "icon": "./media/en.png"
        }
      ]
    },
    "views": {
      "enpractice": [
        {
          "type": "webview",
          "id": "enpractice.practiceView",
          "name": "Practice",
          "when": "true"
        }
      ]
    },
    "commands": [
      {
        "command": "enpractice.openSettings",
        "title": "Settings",
        "icon": "$(gear)"
      },
      {
        "command": "enpractice.openAnalytics",
        "title": "Records",
        "icon": "$(history)"
      },
      {
        "command": "enpractice.openDataAnalysis",
        "title": "Analysis",
        "icon": "$(graph)"
      },
      {
        "command": "enpractice.openDataViewer",
        "title": "Data Viewer",
        "icon": "$(database)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "enpractice.openSettings",
          "when": "view == enpractice.practiceView",
          "group": "navigation"
        },
        {
          "command": "enpractice.openAnalytics",
          "when": "view == enpractice.practiceView",
          "group": "navigation"
        },
        {
          "command": "enpractice.openDataAnalysis",
          "when": "view == enpractice.practiceView",
          "group": "navigation"
        },
        {
          "command": "enpractice.openDataViewer",
          "when": "view == enpractice.practiceView",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "package": "vsce package",
    "build": "npm run compile && npm run package"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  }
}