{
  "name": "enpractice",
  "displayName": "EnPractice",
  "description": "English Practice Extension for VS Code",
  "version": "0.0.1",
  "publisher": "your-publisher-name",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "enpractice",
          "title": "English Practice",
          "icon": "./media/en.png"
        }
      ]
    },
    "views": {
      "enpractice": [
        {
          "type": "webview",
          "id": "enpractice.practiceView",
          "name": "Practice",
          "when": "true"
        }
      ]
    },
    "commands": [
      {
        "command": "enpractice.openSettings",
        "title": "Settings",
        "icon": "$(gear)"
      },
      {
        "command": "enpractice.openAnalytics",
        "title": "Data Analytics",
        "icon": "./media/analyze.png"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "enpractice.openSettings",
          "when": "view == enpractice.practiceView",
          "group": "navigation"
        },
        {
          "command": "enpractice.openAnalytics",
          "when": "view == enpractice.practiceView",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  }
}